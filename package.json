{
  "name": "forrest_rfid",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "dist": "npm run build && electron-builder",
    "preview": "vite preview",
    "electron:start": "electron .",
    "electron:dev": "concurrently \"cross-env DEV=1 vite\" \"cross-env DEV=1 electron .\""
  },
  "build": {
    "appId": "com.vezles.forrestrfid",
    "productName": "Forrest RFID System",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "main.js",
      "preload.js",
      "package.json"
    ],
    "win": {
      "target": "nsis"
    }
  },
  "dependencies": {
    "better-sqlite3": "^12.2.0",
    "dayjs": "^1.11.18",
    "html2canvas": "^1.4.1",
    "pinia": "^3.0.3",
    "preline": "^3.2.3",
    "primeicons": "^7.0.0",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.12",
    "@vitejs/plugin-vue": "^6.0.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "cross-env": "^10.0.0",
    "electron": "^37.2.6",
    "electron-builder": "^26.0.12",
    "electron-rebuild": "^3.2.9",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.12",
    "vite": "^7.1.2"
  }
}
